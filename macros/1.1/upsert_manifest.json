{
    "@context": "https://xshl.org/schemas/1.1/definitions/macros.json",
    "title": "Person name",
    "name": "upsert-manifest",
    "version": "0.0.1",
    "description": "Loading file, validating received data, creating database record",
    "definitions": {
        "url_manifest_schema": "https://xshl.org/schemas/1.1/definitions/manifest.json"
    },
    "pipeline": [
        "upsert:manifest@context/wget/url_manifest",
        "upsert:schema@context/wget/url_manifest_schema",
        "build:errors@context/empty/manifest#schema:schema@validate",
        "upsert:_@context/empty/manifest/daubs#rd:manifest.@value@key",
        "build:result@context/empty/manifest#rd:manifest@container",
        "upsert:result@draft#container:manifest@revision"
    ]
}