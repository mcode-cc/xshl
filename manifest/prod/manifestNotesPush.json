{
    "@context": "https://xshl.org/schemas/1.1/definitions/manifest.json",
    "name": "manifest-notes-push",
    "version": "0.0.4",
    "permissions": [
        "hub:roles:be:developer",
        "hub:roles:cannes:user",
        "hub:roles:cannes:project-admin"
    ],
    "subscription": [
        {
            "attributes": {
                "@value": "$public",
                "target": "$target.#dictionary"
            },
            "defaults": {
                "host": "https://core.kp.house"
            },
            "name": "net.kpcdn.cannes.push"
        }
    ],
    "requirements": [
        {
            "spot": "src",
            "base": "media-chunk-content",
            "entity": "1.2.7"
        }
    ]
}
