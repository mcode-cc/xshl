{
    "@context": "https://xshl.org/schemas/1.1/definitions/manifest.json",
    "name": "manifest-drop-public-text",
    "version": "0.0.5",
    "permissions": ["hub:roles:be:developer", "hub:roles:cannes:user"],
    "obligation": [],
    "requirements": [],
    "advice": [
        {
            "@context": {
                "attributes": {
                    "@value": "$target",
                    "pid": "$pid",
                    "subscriber": "$target.base"
                },
                "defaults": {
                    "event": "deleted"
                }
            },
            "@type": "/extension",
            "base": "boom",
            "entity": "net.kpcdn.private.notify",
            "spot": "send"
        },
        "delete:cos@s3"
    ]
}
